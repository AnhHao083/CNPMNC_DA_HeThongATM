<body class="bg-dark">

    <div class="sufee-login d-flex align-content-center flex-wrap">
        <div class="container">
            <div class="login-content">
                <div class="login-logo">
                    <a href="index.html">
                        <img class="align-content" src="images/logo.png" alt="">
                    </a>
                </div>
                <div class="login-form">
                    <form>
                        <div class="form-group">
                            <label>Email nhân viên</label>
                            <input type="email" class="form-control" placeholder="Email nhân viên">
                        </div>
                        <div class="form-group">
                            <label>Mật khẩu nhân viên</label>
                            <input type="password" class="form-control" placeholder="Mật khẩu nhân viên">
                        </div>
                        <div class="checkbox">
                            <label>
                                <input type="checkbox"> Hãy nhớ tôi
                            </label>
                            <label class="pull-right">
                                <a href="#">Bạn quên mật khẩu?</a>
                            </label>

                        </div>
                        <button type="submit" class="btn btn-success btn-flat m-b-30 m-t-30">Đăng nhập</button>
                        <div class="social-login-content">
                
                        </div>
                        <div class="register-link m-t-15 text-center">
                            <p>Liên hệ admin nếu không có tài khoản.   <a href="#">Liên hệ tại đây</a></p>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/jquery@2.2.4/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.4/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/js/bootstrap.min.js"></script>
