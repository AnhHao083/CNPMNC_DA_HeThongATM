@using Web_CNPMNC_DA_HeThongATM.Controllers

@model CustommerViewModel
@{
    Layout = "_LayoutMain";
    var custommerList = ViewData["j"] as List<CustommerViewModel>;
}
<body>
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <strong class="card-title">Danh sách khách hàng</strong>
            </div>
            <div class="table-stats order-table ov-h">
                <table class="table ">
                    <thead>
                        <tr>
                            <th style="width:10%">Mã</th>
                            <th style="width:15%">Tên</th>
                            <th style="width:15%">CCCD</th>
                            <th style="width:15%%">Email</th>
                            <th style="width:10%">SDT</th>
                            <th style="width:15%%">Giới Tính</th>
                            <th style="width:20%;text-align:left">Địa chỉ</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in custommerList){
                        <tr>
                            <tr>
                                <td>@item.Makh</td>
                                <td>@item.Tenkh</td>
                                <td>@item.CCCD</td>
                                <td>@item.Email</td>
                                <td>@item.Sdt</td>
                                <td>@item.Gioitinh</td>
                                <td>@item.Diachi</td>
                            </tr>
                        </tr>
                        }
                    </tbody>
                </table>
            </div> <!-- /.table-stats -->
        </div>
    </div>
</body>
